{
  "name": "Google Calendar Continuous Month View",
  "version": "0.0",
  "description": "Shortcut to week-increment the custom view for Google Calendar.",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs", "input"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "commands": {
    "down-week": {
      "suggested_key": { "default": "Ctrl+Down" },
      "description": "Move custom-view forward one week"
    },
    "up-week": {
      "suggested_key": { "default": "Ctrl+Up" },
      "description": "Move custom-view backward one week"
    }
  },
  "manifest_version": 2
}
