{
  "name": "Week Increment for Google Calendar Custom View",
  "version": "0.1",
  "description": "Shortcut to week-increment the custom week view for Google Calendar.",
  "permissions": ["activeTab", "declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "commands": {
    "down-week": {
      "suggested_key": { "default": "Ctrl+Down" },
      "description": "Move custom-view forward one week"
    },
    "up-week": {
      "suggested_key": { "default": "Ctrl+Up" },
      "description": "Move custom-view backward one week"
    }
  },
  "manifest_version": 2
}
